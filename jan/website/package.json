    "astro-mermaid": "^1.0.4",
    "mermaid": "^11.9.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sharp": "^0.34.3",
    "starlight-openapi": "^0.19.1",
    "starlight-sidebar-topics": "^0.6.0",
