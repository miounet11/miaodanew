{
  "name": "@miaoda/extensions-web",
  "version": "1.0.0",
  "description": "Web-specific extensions for Miaoda",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "dev": "tsc --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@miaoda/core": "workspace:*",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vitest": "^2.0.0",
    "zustand": "^5.0.8"
  },
  "peerDependencies": {
    "@miaoda/core": "*",
    "zustand": "^5.0.0"
  }
}
